{
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "generator-star-spacing": [
      2,
      "before"
    ],
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-underscore-dangle": 0,
    "no-cond-assign": [
      2,
      "always"
    ],
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-reserved-keys": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "block-scoped-var": 2,
    "consistent-return": 1,
    "curly": 2,
    "default-case": 2,
    "dot-location": [
      2,
      "property"
    ],
    "eqeqeq": [
      1,
      "smart"
    ],
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 0,
    "no-empty-label": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 1,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 1,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 1,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-process-env": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 0,
    "wrap-iife": 2,
    "yoda": [
      2,
      "never",
      {
        "exceptRange": true
      }
    ],
    "strict": [
      2,
      "global"
    ],
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 1,
    "no-undef": 2,
    "no-undefined": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "no-new-require": 2,
    "no-path-concat": 1,
    "no-process-exit": 1,
    "no-restricted-modules": [
      1,
      ""
    ],
    "no-sync": 1,
    "brace-style": [
      2,
      "1tbs"
    ],
    "camelcase": [
      "error",
      {
        "properties": "always"
      }
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "consistent-this": [
      1,
      "_this"
    ],
    "eol-last": 2,
    "func-names": 0,
    "func-style": 0,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "key-spacing": 0,
    "linebreak-style": [
      2,
      "unix"
    ],
    "max-nested-callbacks": [
      2,
      5
    ],
    "new-parens": 2,
    "new-cap": 0,
    "sort-imports": [
      "error",
      {
        "ignoreCase": false,
        "ignoreMemberSort": false,
        "memberSyntaxSortOrder": [
          "none",
          "all",
          "multiple",
          "single"
        ]
      }
    ]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "generators": true
    },
    "sourceType": "module"
  }
}
