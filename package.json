{
  "name": "Real_Pitch",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start-with-cache": "react-native start",
    "start": "./node_modules/react-native/packager/packager.sh start --reset-cache",
    "clean": "find ./node_modules -name react-packager -prune -o -name '.babelrc' -print | xargs rm -f",
    "postinstall": "npm run clean",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register -c -t 10000 --check-leaks ./src/test",
    "test-cov": "./node_modules/.bin/istanbul --config=./.istanbul.yml cover ./node_modules/.bin/_mocha -- --compilers js:babel-core/register -R spec src/test"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "co": "^4.6.0",
    "lodash": "^4.3.0",
    "react": "^15.3.1",
    "react-native": "^0.33.0",
    "react-native-scrollable-tab-view": "^0.5.0",
    "react-redux": "^4.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "debug": "^2.0.0",
    "eslint": "^2.6.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
